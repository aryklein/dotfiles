[credential]
	helper = cache --timeout=3600
[alias]
	ql = log -n15 --topo-order --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
[pull]
	rebase = false
[init]
	defaultBranch = main
